<h1>ユーザー 編集</h1>
<%= form_for @user,url: user_path, html: {method: :patch} do |f| %>
  <div class="form-group">
    氏名 <%= f.text_field :name %>
  </div>
  <div class="form-group">
    年齢 <%= f.text_field :age %>
  </div>
  <input type="submit" value="送信">
<% end %>
<%= link_to "一覧画面へ", users_path %>